{
  "parameters": {
    "db": {
      "host": "mongodb",
      "port": 27017,
      "database": "test"
    },
    "collection": "restaurants",
    "query": "{name: \"National Bakery\"}",
    "sort": "{\"_id\": 1}",
    "limit": 3,
    "tableName": "export-multi-with-sort-and-limit",
    "mapping": {
      "_id.$oid": {
        "type": "column",
        "mapping": {
          "destination": "id"
        }
      },
      "name": {
        "type": "column",
        "mapping": {
          "destination": "name"
        }
      }
    },
    "mode": "mapping"
  }
}