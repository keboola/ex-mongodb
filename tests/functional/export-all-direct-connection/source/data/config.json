{
  "parameters": {
    "db": {
      "host": "mongodb",
      "port": 27017,
      "database": "test"
    },
    "collection": "restaurants",
    "tableName": "export-all",
    "mapping": {
      "_id.$oid": {
        "type": "column",
        "mapping": {
          "destination": "id"
        }
      },
      "name": {
        "type": "column",
        "mapping": {
          "destination": "name"
        }
      }
    },
    "mode": "mapping",
    "sort": "{_id: 1}"
  }
}