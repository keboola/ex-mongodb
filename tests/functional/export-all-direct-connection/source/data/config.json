{
  "parameters": {
    "db": {
      "host": "mongodb",
      "port": 27017,
      "database": "test"
    },
    "export": {
      "collection": "restaurants",
      "name": "export-all",
      "mapping": {
        "_id.$oid": {
          "type": "column",
          "mapping": {
            "destination": "id"
          }
        },
        "name": {
          "type": "column",
          "mapping": {
            "destination": "name"
          }
        }
      },
      "enabled": true,
      "mode": "mapping",
      "sort": "{_id: 1}"
    }
  }
}