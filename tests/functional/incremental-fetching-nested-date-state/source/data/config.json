{
  "parameters": {
    "db": {
      "host": "mongodb",
      "port": 27017,
      "database": "test"
    },
    "exports": [
      {
        "name": "incremental",
        "id": "export-id",
        "collection": "incremental",
        "incremental": true,
        "incrementalFetchingColumn": "nested.date",
        "mapping": {
          "nested": {
            "type": "table",
            "destination": "table_nested",
            "parentKey": {
              "disable": true
            },
            "tableMapping": {
              "date.$date": "value"
            }
          }
        }
      }
    ]
  }
}