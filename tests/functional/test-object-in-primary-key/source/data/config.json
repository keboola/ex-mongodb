{
  "parameters": {
    "db": {
      "host": "mongodb",
      "port": 27017,
      "database": "test"
    },
    "export": {
      "name": "object-in-primary-key",
      "id": 123,
      "enabled": true,
      "collection": "restaurants",
      "incremental": true,
      "mode": "mapping",
      "mapping": {
        "_id": {
          "type": "column",
          "mapping": {
            "destination": "id",
            "primaryKey": true
          }
        },
        "coord": {
          "type": "table",
          "destination": "coord",
          "tableMapping": {
            "0": "lat",
            "1": "lat"
          }
        }
      }
    }
  }
}